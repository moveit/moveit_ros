<?xml version="1.0"?>
<launch>
  <arg name="ip_str" value="127.0.0.1" />


  <group ns="load_m10ia7l__">
    <include file="$(find test_robot_support)/launch/load_m10ia7l.launch"/>
  </group>

  <group ns="test_m10ia7l__">
    <include file="$(find test_robot_support)/launch/test_m10ia7l.launch"/>
  </group>

  <group ns="robot_interface_streaming_m10ia7l__">
    <include file="$(find test_robot_support)/launch/robot_interface_streaming_m10ia7l.launch">
      <arg name="robot_ip" value="$(arg ip_str)" />
    </include>
  </group>

  <group ns="robot_state_visualize_m10ia7l__">
    <include file="$(find test_robot_support)/launch/robot_state_visualize_m10ia7l.launch">
      <arg name="robot_ip" value="$(arg ip_str)" />
    </include>
  </group>


  <!-- without bswap -->
  <group ns="robot_interface_streaming_m10ia7l_f__">
    <include file="$(find test_robot_support)/launch/robot_interface_streaming_m10ia7l.launch">
      <arg name="robot_ip" value="$(arg ip_str)" />
      <arg name="use_bswap" value="false" />
    </include>
  </group>

  <group ns="robot_state_visualize_m10ia7l_f__">
    <include file="$(find test_robot_support)/launch/robot_state_visualize_m10ia7l.launch">
      <arg name="robot_ip" value="$(arg ip_str)" />
      <arg name="use_bswap" value="false" />
    </include>
  </group>
</launch>
